{
	"compileOnSave": true,
	"compilerOptions": {
		"alwaysStrict": true,
		"lib": ["esnext"],
		"module": "commonjs",
		"noUnusedParameters": true,
		"sourceMap": true,
		"moduleResolution": "node",
		"noUnusedLocals": true,
		"removeComments": false,
		"target": "ES2020",
		"importsNotUsedAsValues": "error",
		"strictNullChecks": true,
		"outDir": "dist"
	},
	"exclude": [
		"node_modules"
	]
}